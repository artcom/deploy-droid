{
  "name": "deploy-droid",
  "version": "0.2.0",
  "description": "Automated deployment for HockeyApp Android applications",
  "repository": "artcom/deploy-droid",
  "files": [
    "dist/"
  ],
  "bin": {
    "dock": "dist/main.js"
  },
  "scripts": {
    "prepublish": "babel src --out-dir dist --source-maps",
