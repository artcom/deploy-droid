    "colors": "^1.1.2",
    "docopt": "^0.6.2",
    "lodash": "^3.10.1",
    "log-update": "^1.0.2",
    "read": "^1.0.7",
    "text-table": "^0.2.0",
    "wgetjs": "^0.3.6",
    "yn": "^1.2.0"
  },
  "devDependencies": {
    "babel": "^5.8.29",
    "babel-eslint": "^4.1.4",
    "eslint": "^1.8.0"
  },
  "license": "MIT"
}
